APP_ID=<YOUR_APP_ID>
DISCORD_TOKEN=<YOUR_BOT_TOKEN>
PUBLIC_KEY=<YOUR_PUBLIC_KEY>
FANSLY_TOKEN=<YOUR_FANSLY_TOKEN>
USER_AGENT=<YOUR_BROWSER_USER_AGENT>
LOG_CHANNEL_ID=<CHANNEL_ID_TO_LOG_ADDED_CREATORS>

# Database Configuration
# Options: "sqlite" or "postgres"
DB_TYPE=sqlite

# SQLite Configuration (used when DB_TYPE=sqlite)
SQLITE_PATH=bot.db

# PostgreSQL Configuration (used when DB_TYPE=postgres)
POSTGRES_URL=postgres://username:password@localhost:5432/fansly_notify

# Debug mode
DEBUG=false
